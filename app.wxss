/**app.wxss**/
page{
	background: #f1f1f1;
}
/*设置公共背景色，margin值*/
.bgv {
   background-color:#f1f1f1;
} 
.mt20{
	margin-top:20rpx;
}
/*封装一个做间隔的类,设定高度，填充颜色*/

.divider{
	height:20rpx;
	background-color:#f1f1f1;
	box-shadow:0 5rpx 5rpx rgba(0,0,0,0.1);
}
/*封装边框细线*/
.bdr{
	position: relative;
}
.bdr::after{
	content: '';
	position: absolute;
	width: 1rpx;
	height: 100%;
	background-color: #f00;
	right:0;
	top:0;
}
/* 箭头 */
.arrow{
	display:flex;
	justify-content:space-between;
	align-items:center;
}
.arrow::after{
	content:"";
	width:20rpx;
	height:20rpx;
	border-top: 1rpx solid #ccc ;
	border-right: 1rpx solid #ccc;
  /* display: block; */
  transform: rotate(45deg);
}
/* 居中 */
.center{
	display: flex;
	justify-content: center;
	align-items: center;
}
/* 列表封装 */
.cell{
  font-size: 14px;
  background-color:#fff;
}
.cell-item{
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #ddd;
}

.bdr1{
	position: relative;
}
.bdr1::after{
	content: '';
	position: absolute;
	width: 1rpx;
	height: 100%;
	background-color: #f00;
	right:0;
	top:0;
}
/*星星评分封装*/
.star-before{
	position: absolute;
	top: 0;
	left: 0;
	color: #f19e38;
	width: 90%;
	overflow: hidden;
}
.star-after{
	 color:#e2e2e2;
}
/*封装省略号*/
.ell{
	overflow:hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
/* 封装loading */
.loadmore{
	color:#888;
	font-size:14px;
	line-height:100rpx;
	text-align:center;
	}
.loadmore.loading::before{
	content:'';
	width:40rpx;
	height:40rpx;
	margin-top:-10rpx;
	display:inline-block;
	vertical-align:middle;
	animation:loading 1s steps(12) infinite;
	background:transparent url(data:image/png;base64,) no-repeat;
	background-size:100%;
	
 }
 @keyframes loading{
	from{
	 transform:rotate(0deg);
	}
	to{
	transform:rotate(360deg);
	}
 }